generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id                     String  @id @default(uuid())
  address                String  @unique
  sumSubStatus           String  @default("none")
  loginMessage           String?
  loginSignature         String?
  idosPubKey             String?
  idosGrantOwner         String?
  idosGrantGrantee       String?
  idosGrantDataId        String?
  idosGrantLockedUntil   String?
  idosGrantMessage       String?
  idosGrantSignature     String?
  idosGrantTransactionId String?
  idosHumanId            String?
  idosWalletId           String?
  idosCredentialId       String?
}

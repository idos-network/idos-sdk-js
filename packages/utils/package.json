{
  "name": "@idos-network/utils",
  "description": "idOS Utils JavaScript SDK for enclave environments",
  "version": "1.0.0-rc.1.1",
  "dependencies": {
    "@idos-network/credentials": "workspace:*",
    "@stablelib/base64": "catalog:utils",
    "@stablelib/binary": "catalog:utils",
    "@stablelib/bytes": "catalog:utils",
    "@stablelib/hex": "catalog:utils",
    "@stablelib/sha256": "catalog:utils",
    "@stablelib/utf8": "catalog:utils",
    "borsh": "catalog:utils",
    "bs58": "catalog:utils",
    "scrypt-js": "^3.0.1",
    "tweetnacl": "^1.0.3",
    "web-bip39": "^0.0.3",
    "xrpl": "catalog:xrp",
    "zod": "catalog:core"
  },
  "devDependencies": {
    "@types/chrome": "^0.1.0",
    "@types/node": "^22.15.30",
    "jsdom": "^22.1.0",
    "tiny-invariant": "catalog:core",
    "tsdown": "catalog:build",
    "typescript": "catalog:build",
    "vitest": "catalog:testing"
  },
  "engines": {
    "node": ">=24.12.0"
  },
  "exports": {
    "./encryption": {
      "types": "./dist/encryption/index.d.mts",
      "import": "./dist/encryption/index.mjs"
    },
    "./store": {
      "types": "./dist/store/index.d.mts",
      "import": "./dist/store/index.mjs"
    },
    "./codecs": {
      "types": "./dist/codecs/index.d.mts",
      "import": "./dist/codecs/index.mjs"
    },
    "./enclave": {
      "types": "./dist/enclave/index.d.mts",
      "import": "./dist/enclave/index.mjs"
    },
    "./enclave/local": {
      "types": "./dist/enclave/local.d.mts",
      "import": "./dist/enclave/local.mjs"
    },
    "./xrpl-credentials": {
      "types": "./dist/xrpl-credentials/index.d.mts",
      "import": "./dist/xrpl-credentials/index.mjs"
    },
    "./mpc": {
      "types": "./dist/mpc/index.d.mts",
      "import": "./dist/mpc/index.mjs"
    }
  },
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "homepage": "https://idos.network",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "dist/index.js",
  "peerDependencies": {
    "@partisiablockchain/abi-client": "^5.207.0",
    "@partisiablockchain/blockchain-api-transaction-client": "^5.149.0",
    "ethers": "catalog:ethereum",
    "typescript": "catalog:build"
  },
  "peerDependenciesMeta": {
    "@partisiablockchain/blockchain-api-transaction-client": {
      "optional": true
    },
    "@partisiablockchain/abi-client": {
      "optional": true
    },
    "ethers": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/idos-network/idos-sdk-js",
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown  --watch",
    "publint": "pnpx publint --strict",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "sideEffects": false,
  "type": "module",
  "types": "./dist/index.d.ts"
}

{
  "name": "@idos-network/encryption",
  "description": "idOS Encryption JavaScript SDK for enclave environments",
  "version": "1.0.0-rc.1.0",
  "dependencies": {
    "@stablelib/utf8": "^2.0.1",
    "@stablelib/base64": "^2.0.1",
    "scrypt-js": "^3.0.1",
    "tweetnacl": "^1.0.3",
    "web-bip39": "^0.0.3"
  },
  "devDependencies": {
    "release-it": "^19.0.2",
    "release-it-pnpm": "^4.6.5",
    "tiny-invariant": "catalog:core",
    "typescript": "catalog:build",
    "tsdown": "catalog:build"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "exports": {

{
  "rewrites": [{ "source": "/(.*)", "destination": "/" }],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "" },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors https://*.idos.network https://localhost:*"
        }
      ]
    }
  ]
}
